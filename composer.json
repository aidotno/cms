{
    "name": "devisephp/cms",
    "description": "Content Management System riding on top of the Laravel Framework",
    "keywords": ["cms", "devise", "devisephp", "laravel", "content management system"],
    "license": "MIT",
    "authors": [
        {
            "name": "DevisePHP Team",
            "email": "info@devisephp.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.6.*",
        "guzzlehttp/guzzle": "~6.0",
        "kg-bot/laravel-localization-to-vue": "^1.5",
        "spatie/laravel-db-snapshots": "^1.1",
        "spatie/laravel-glide": "^3.2",
        "league/glide-laravel": "^1.0",
        "spatie/laravel-image-optimizer": "^1.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-0": {
            "Devise\\": "Devise/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devise\\Providers\\DeviseServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
